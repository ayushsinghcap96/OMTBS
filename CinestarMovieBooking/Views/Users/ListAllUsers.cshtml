@model IEnumerable<CinestarEntities.UserEntity>

@{
    ViewBag.Title = "ListAllUsers";
}

<h2>ListAllUsers</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.ActionLink("Details", "ProfileDetails", "ViewerProfiles", new { id=item.UserName }, null) 
            @*@Html.ActionLink("Details", "ProfileDetails", "ViewerProfiles", new { id = item.UserName }) |*@
        </td>
    </tr>
}

</table>
